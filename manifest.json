{
  "manifest_version": 3,
  "name": "archrome",
  "version": "0.1.0",
  "description": "A Chrome extension to manage browser spaces using bookmarks.",
  "permissions": [
    "bookmarks",
    "tabs",
    "storage",
    "sidePanel",
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_title": "Open archrome sidebar",
    "default_icon": {
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidebar.html"
  },
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+Q",
        "mac": "Alt+Q"
      },
      "description": "Toggle the archrome sidebar"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["icons/default_favicon.png"],
      "matches": ["<all_urls>"]
    }
  ]
}